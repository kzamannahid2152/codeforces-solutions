#include<bits/stdc++.h>
using namespace std;

int main(){

     int n;
     cin>>n;

     int array[n];
     for(int i=0; i<n; i++){
        cin>>array[i];
     }
      
     //search_number; 
     int x;
     cin>>x;
        for(int i=0; i<n; i++){
            if(x==array[i]){ //cmpr;
            cout<<i<<endl; //index printing;
            return 0;
        }
    }
    cout<<-1<<endl;

     return 0;
}
