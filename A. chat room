#include<bits/stdc++.h>
using namespace std;

int main()
{
    string str;
    cin>>str;
    string str2 = "hello";
    int x=0,  count=0;

    for(int i=0; i<str.size(); i++){
        if(str[i]==str2[x]){
            x++;
            count++;
        }
    }

    if(count==5){
        cout<<"YES"<<endl;
    }
    else{
        cout<<"NO"<<endl;
    }

    return 0;
}
