#include<bits/stdc++.h>
using namespace std;

int main()
{
    int t;
    cin>>t;

    while(t--)
    {

        int n;
        cin>>n;
        int c1_c2= n/3;
        int rem = n%3;
        int c1 = c1_c2;
        int c2 = c1_c2;
        if(rem==1)
            c1++;
        if(rem==2)
            c2++;
        cout<<c1<<" "<<c2<<endl;
    }
    return 0;
}
